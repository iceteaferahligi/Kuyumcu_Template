            document.getElementById("tahsilat").innerHTML = "";
        } else if (document.getElementById('tutar').value < 0) {
            document.getElementById("tahsilat").innerHTML = "ÖDEYİNİZ.";
        } else {
            document.getElementById("tahsilat").innerHTML = "TAHSİL EDİNİZ.";
        }
    }

